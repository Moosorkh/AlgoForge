{
  "name": "algoforge-backend",
  "version": "1.0.0",
  "description": "Backend API for AlgoForge",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "node dist/database/migrate.js",
    "db:setup": "ts-node src/setup-mysql.ts",
    "db:seed": "ts-node src/load-seed.ts",
    "db:check": "ts-node src/check-data.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "mysql2": "^3.15.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}